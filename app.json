{
  "expo": {
    "name": "Foodie",
    "slug": "Fooddelivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "foodie",

    "icon": "./assets/foodie-icon.png",

    "splash": {
      "image": "./assets/foodie-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#111827"
    },

    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yognik.fooddelivery",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.yognik.fooddelivery",
      "adaptiveIcon": {
        "foregroundImage": "./assets/foodie-adaptive-foreground.png",
        "backgroundImage": "./assets/foodie-adaptive-background.png",
        "foregroundImageTint": null
      },
      "edgeToEdgeEnabled": true,
      "softwareKeyboardLayoutMode": "pan"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/foodie-splash.png",
          "imageWidth": 240,
          "resizeMode": "contain",
          "backgroundColor": "#111827"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true
    },

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d935a3cc-463b-409e-8b40-e9ae247e5718"
      },
      "BACKEND_URL": "https://otp-backend1.onrender.com",
      "experiments": { "typedRoutes": true }
    }
  }
}
